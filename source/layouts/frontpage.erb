<!doctype html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv='X-UA-Compatible' content='IE=edge;chrome=1' />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Nick Benoit</title>
    <%= feed_tag :atom, "#{blog.options.prefix.to_s}/feed.xml", title: "Atom Feed" %>
    <%= stylesheet_link_tag :site %>
    <%= stylesheet_link_tag "bootstrap.min" %>
    <%= stylesheet_link_tag "font-awesome-4.6.3/css/font-awesome.min" %>
  </head>
  <body>
    <div class="c-frontpage">
    <header>
      <div>
        <h3>Nick Benoit - Sample intro text</h3>
        <div>
          <%= link_to 'Resume', '#' %>
          <%= link_to 'Projects', '/projects/index.html'%>
          <%= link_to 'Blog', 'blog/index.html'%>
        </div>
      </div>
    </header>
    <div id="main" role="main">
      <%= yield %>
    </div>
    <%= javascript_include_tag :all %>
    </div>
    <%= partial "footer" %>
  </body>
  </html>
